BIN_DIR="/home/kms/KMS/PKG_R2.0.2/bin"
BIN_LIST="sim_qkd_skt sim_qkd_skt2 sim_qkd_skt3 sim_qkd_skt4 "

CORE_SO_DIR="/home/kms/KMS/PKG_R2.0.2/lib"

if [ $# != 1 ] ; then
    echo "rchg [proc_name | all ]"
    exit
fi

if [ $1 = "all" ] ; then
    for item in $BIN_LIST
    do  
	    ./kill.sh 
        sudo scp kms@dev:$BIN_DIR/$item ./
		sudo chown kms:kms $item
	    ./run.sh 
    done
else
	./kill.sh 
    sudo scp kms@dev:$BIN_DIR/$1 ./
	sudo chown kms:kms ./$1
	./run.sh 
fi
